{
	"bot": {
		"rateLimits": "regular",
		"entryChannelLogin": "",
		"login": "",
		"id": "0"
	},
	"commands": {
		"defaultPrefix": "!",
		"loadUnsafe": true,
		"suggestClosest": true
	},
	"messages": {
		"tosViolationPlaceholder": "message goes against twitch TOS",
		"responsePartsSeparator": " Â· ",
		"logByDefault": true
	},
	"twitch": {
		"ircTransport": "websocket",
		"sender": {
			"transport": "irc",
			"irc": {
				"connectionsPoolSize": 10
			}
		},
		"hermes": {
			"maxConnections": 20,
			"maxTopicsPerConnection": 50,
			"autoAcknowledgeChatWarnings": true,
			"autoJoinRaids": true,
			"autoJoinWatching": true
		}
	},
	"retry": {
		"maxRetries": 3,
		"baseDelayMs": 200,
		"jitter": 0.5
	},
	"cache": "redis",
	"db": {
		"messagesFlushIntervalMs": 2500,
		"maxMessagesPerChannelFlush": 150
	},
	"hastebin": {
		"instance": "https://paste.ivr.fi",
		"maxPasteLength": 1500000
	},
	"metrics": {
		"enabled": false,
		"sampleIntervalMs": 5000,
		"logIntervalMs": 1800000,
		"prometheus": {
			"enabled": false,
			"host": "127.0.0.1",
			"port": 9101,
			"endpoint": "/metrics",
			"prefix": "selfbot_"
		}
	},
	"shell": "/bin/bash",
	"logger": {
		"level": "info",
		"colorize": true
	}
}
