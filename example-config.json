{
	"bot": {
		"rateLimits": "regular",
		"entryChannelLogin": "",
		"login": "",
		"id": "0"
	},
	"commands": {
		"defaultPrefix": "!",
		"loadUnsafe": true,
		"suggestClosest": false
	},
	"messages": {
		"tosViolationPlaceholder": "message goes against twitch TOS",
		"responsePartsSeparator": " Â· ",
		"logByDefault": true
	},
	"twitch": {
		"sender": {
			"backend": "irc"
		},
		"irc": {
			"socket": "websocket",
			"maxChannelCountPerConnection": 100,
			"connectionsPoolSize": 10
		},
		"hermes": {
			"maxConnections": 100,
			"maxTopicsPerConnection": 50,
			"autoAcknowledgeChatWarnings": true,
			"autoJoinRaids": true,
			"autoJoinWatching": false,
			"autoBet": {
				"enabled": false,
				"ignoreOwnPredictions": true,
				"minRequiredBalance": 10,
				"strategy": {
					"betDelayPercent": 80,
					"outcomeSelection": "mostPopular",
					"bet": {
						"min": 10,
						"max": 50000,
						"poolFraction": 0.3,
						"maxBalanceFraction": 0.1,
						"onInsufficientFunds": "betAll"
					}
				}
			}
		}
	},
	"retry": {
		"maxRetries": 3,
		"baseDelayMs": 200,
		"jitter": 0.5
	},
	"cache": "redis",
	"db": {
		"messagesFlushIntervalMs": 2500,
		"maxMessagesPerChannelFlush": 150
	},
	"paste": {
		"service": "hastebin",
		"maxLength": 500000,
		"hastebin": {
			"instance": "https://h.roe.lol/hb",
			"raw": true
		},
		"nullPtr": {
			"instance": "https://0x0.st",
			"secret": false
		}
	},
	"metrics": {
		"enabled": false,
		"sampleIntervalMs": 5000,
		"prometheus": {
			"enabled": false,
			"host": "127.0.0.1",
			"port": 9101,
			"endpoint": "/metrics",
			"prefix": "selfbot_"
		}
	},
	"shell": "/bin/bash",
	"logger": {
		"level": "info",
		"showErrorStackTraces": true,
		"colorize": true
	}
}
